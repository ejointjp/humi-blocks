{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "lito/alert",
  "version": "0.1.0",
  "title": "アラート",
  "category": "lito-blocks",
  "icon": "info",
  "description": "アイコンつきのアラート文を作成できます",
  "example": {},
  "attributes": {
    "iconName": {
      "type": "string",
      "default": "info"
    }
  },
  "supports": {
    "html": true,
    "defaultStylePicker": true
  },
  "styles": [
    {
      "name": "info",
      "label": "Info"
    },
    {
      "name": "success",
      "label": "Success"
    },
    {
      "name": "warning",
      "label": "Warning"
    },
    {
      "name": "danger",
      "label": "Danger"
    }
  ],
  "keywords": [
    "コメント",
    "注意",
    "注釈"
  ],
  "textdomain": "lito-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}