{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "humi/marker-text",
  "version": "0.1.0",
  "title": "Humi マーカーテキスト",
  "category": "humi-blocks",
  "icon": "yes",
  "description": "アイコンつきのテキストを作成できます",
  "attributes": {
    "content": {
      "type": "string"
    },
    "iconName": {
      "type": "string",
      "default": "check"
    },
    "color": {
      "type": "string"
    }
  },
  "example": {},
  "supports": {
    "html": false
  },
  "keywords": ["テキスト", "text", "マーカー", "marker", "マーク", "mark", "チェック", "check"],
  "textdomain": "humi-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
