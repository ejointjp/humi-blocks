{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "humi/profile",
  "version": "0.1.0",
  "title": "Humi プロフィール",
  "category": "humi-blocks",
  "icon": "admin-users",
  "description": "講演者などのプロフィールを表示できます。",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "humi-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "avatarID": {
      "type": "number",
      "default": 0
    },
    "avatarURL": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src",
      "default": "https://0.gravatar.com/avatar/00000000000000000000000000000000?s=256&d=mp&r=g"
    },
    "avatarAlt": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "alt",
      "default": ""
    },
    "name": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "source": "html",
      "selector": ".wp-block-humi-profile-description",
      "default": ""
    }
  }
}